def get_target_url():
    print(f"{Fore.CYAN}{'â•' * 60}{Style.RESET_ALL}")
    print(f"{Fore.YELLOW}[ğŸ¯] TARGET INPUT{Style.RESET_ALL}")
    print(f"{Fore.CYAN}{'â•' * 60}{Style.RESET_ALL}\n")
    
    while True:
        target = input(f"{Fore.GREEN}[?] Enter target website URL (e.g., https://example.com): {Style.RESET_ALL}").strip()
        
        if not target:
            print(f"{Fore.RED}[!] URL cannot be empty! Please try again.{Style.RESET_ALL}")
            continue
            
        if not target.startswith(('http://', 'https://')):
            print(f"{Fore.YELLOW}[!] No protocol specified. Adding https://{Style.RESET_ALL}")
            target = 'https://' + target
        
        confirm = input(f"{Fore.YELLOW}[?] Target set to: {Fore.CYAN}{target}{Fore.YELLOW}. Continue? (y/n): {Style.RESET_ALL}").lower()
        if confirm == 'y' or confirm == 'yes':
            return target
        else:
            print(f"{Fore.RED}[!] Please re-enter the target URL.{Style.RESET_ALL}\n")

def check_url(url, timeout=5):
    try:
        response = requests.get(url, timeout=timeout, allow_redirects=True, headers={'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36'})
        if response.status_code == 200:
            return (url, True, response.status_code, "FOUND")
        elif response.status_code == 401:
            return (url, True, response.status_code, "UNAUTHORIZED")
        elif response.status_code == 403:
            return (url, True, response.status_code, "FORBIDDEN")
        else:
            return (url, False, response.status_code, "NOT_FOUND")
    except requests.exceptions.ConnectionError:
        return (url, False, None, "CONNECTION_ERROR")
    except requests.exceptions.Timeout:
        return (url, False, None, "TIMEOUT")
    except requests.exceptions.RequestException:
        return (url, False, None, "ERROR")

def load_admin_paths(filename="admin.txt"):
    try:
        with open(filename, "r") as file:
            paths = [line.strip() for line in file if line.strip()]
        return paths
    except FileNotFoundError:
        print(f"{Fore.RED}[!] File {filename} not found!{Style.RESET_ALL}")
        sys.exit(1)

def print_progress_bar(current, total, start_time):
    percentage = (current / total) * 100
    elapsed = time.time() - start_time
    bar_length = 30
    filled_length = int(bar_length * current // total)
    bar = f"{Fore.GREEN}â–ˆ{Style.RESET_ALL}" * filled_length + f"{Fore.RED}â–’{Style.RESET_ALL}" * (bar_length - filled_length)
    
    if current > 0:
        eta = (elapsed / current) * (total - current)
        eta_str = time.strftime("%H:%M:%S", time.gmtime(eta))
    else:
        eta_str = "N/A"
    
    print(f"\r{Fore.CYAN}[*] Progress: |{bar}| {Fore.YELLOW}{percentage:.1f}%{Style.RESET_ALL} {Fore.MAGENTA}[{current}/{total}]{Style.RESET_ALL} {Fore.BLUE}ETA: {eta_str}{Style.RESET_ALL}", end="")

